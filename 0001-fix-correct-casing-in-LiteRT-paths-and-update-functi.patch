From fffcb7c1dc6d268e94d0857d1479ada7361ddba0 Mon Sep 17 00:00:00 2001
From: GeonhaPark <geonhab504@gmail.com>
Date: Wed, 2 Jul 2025 20:17:10 +0900
Subject: [PATCH] fix: correct casing in LiteRT paths and update function
 declaration in util.hpp

---
 .env                    | 2 +-
 scripts/build-litert.sh | 4 ++--
 setup.sh                | 2 +-
 src/util.hpp            | 2 +-
 4 files changed, 5 insertions(+), 5 deletions(-)

diff --git a/.env b/.env
index 89f32a6..9a772f7 100644
--- a/.env
+++ b/.env
@@ -1,3 +1,3 @@
 ROOT_PATH=$(pwd)
 EXTERNAL_PATH="${ROOT_PATH}/external" # default
-LITERT_PATH="${EXTERNAL_PATH}/LiteRT"  # default
\ No newline at end of file
+LITERT_PATH="${EXTERNAL_PATH}/litert"  # default
\ No newline at end of file
diff --git a/scripts/build-litert.sh b/scripts/build-litert.sh
index fa3df10..69df2c8 100755
--- a/scripts/build-litert.sh
+++ b/scripts/build-litert.sh
@@ -5,8 +5,8 @@ cd ..
 source .env
 
 # ── Build Configuration ───────────────────────────────────────────────────────
-FLATBUFFER_PATH=${LITERT_PATH}/bazel-LiteRT/external/flatbuffers/include/flatbuffers
-TENSORFLOW_PATH=${LITERT_PATH}/bazel-LiteRT/external/org_tensorflow/tensorflow
+FLATBUFFER_PATH=${LITERT_PATH}/bazel-litert/external/flatbuffers/include/flatbuffers
+TENSORFLOW_PATH=${LITERT_PATH}/bazel-litert/external/org_tensorflow/tensorflow
 
 LITERT_LIB_PATH=${LITERT_PATH}/bazel-bin/tflite/libtensorflowlite.so
 
diff --git a/setup.sh b/setup.sh
index a347d3b..0896177 100755
--- a/setup.sh
+++ b/setup.sh
@@ -37,7 +37,7 @@ pwd
 
 ## Clone LiteRT
 echo "[INFO] Installing LiteRT"
-if [ ! -d "./LiteRT" ]; then
+if [ ! -d "./litert" ]; then
     git clone https://github.com/google-ai-edge/litert.git
     cd ${LITERT_PATH}
     ./configure
diff --git a/src/util.hpp b/src/util.hpp
index c423cf5..e587816 100644
--- a/src/util.hpp
+++ b/src/util.hpp
@@ -54,7 +54,7 @@ namespace util
     void softmax(const float *logits, std::vector<float> &probs, int size);
 
     // Preprocess image to match model input size
-    cv::Mat util::preprocess_image(cv::Mat &image, int target_height, int target_width);
+    cv::Mat preprocess_image(cv::Mat &image, int target_height, int target_width);
 
 } // namespace util
 
-- 
